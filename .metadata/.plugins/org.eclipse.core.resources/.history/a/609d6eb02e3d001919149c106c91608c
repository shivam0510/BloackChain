<html>

<script type="text/javascript">
function buyItemList(){
	document.getElementById('index').action = "buyItemList";
	document.getElementById('index').submit();
}

function sellItem(){
	document.getElementById('index').action = "loginPage";
	document.getElementById('index').submit();
}
</script>


